<?xml version="1.0"?>
<!DOCTYPE PRODUCTION_ORDERS [
<!ELEMENT ORDERS (Request_Stores | Order*)>
<!ELEMENT Request_Stores EMPTY>
<!ELEMENT Order (Transform | Unload)>
<!ATTLIST Order
          Number   (CDATA) #REQUIRED
>
<!ELEMENT Transform EMPTY>
<!ATTLIST Transform
          From     (CDATA) #REQUIRED
          To       (CDATA) #REQUIRED
          Quantity (CDATA) #REQUIRED
          MaxDelay (CDATA) #REQUIRED
>
<!ELEMENT Unload EMPTY>
<!ATTLIST Unload
          Type        (CDATA) #REQUIRED
          Destination (CDATA) #REQUIRED
          Quantity    (CDATA) #REQUIRED
>
]>
<ORDERS>
<Order Number="002">
<Transform From="P3" To="P5" Quantity="7" MaxDelay="300"/>
</Order>
<Order Number="003">
<Transform From="P7" To="P9" Quantity="10" MaxDelay="300"/>
</Order>
</ORDERS>
